{
  "name": "agricola-marks",
  "version": "1.5.1",
  "private": true,
  "description": "Agricola Score Caliculator",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "3.6.0",
    "@material-ui/icons": "4.5.1",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-smooth-scroller": "1.2.0",
    "store": "2.0.12"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "react-scripts": "3.3.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
